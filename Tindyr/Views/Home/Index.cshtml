@{
    ViewData["Title"] = "Home Page";
}

@if (!User.Identity.IsAuthenticated)
{
    @await Html.PartialAsync("_LandingPageNewUser")

}
else
{
    @await Html.PartialAsync("_LandingPageExistingUser")
}

@section scripts{
    <script type="text/javascript">
                      function DoComfirm()
                        {
                                    window.location.href = "@Url.Action("Register", "Auth")";
                        }
    </script>
}



